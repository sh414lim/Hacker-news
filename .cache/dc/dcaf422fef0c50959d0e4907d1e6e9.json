();\n    \n    const newsCotent=JSON.parse(ajax.response);\n    const title = document.createElement('h1');\n\n    title.innerHTML=newsCotent.title;\n\n    content.appendChild(title);\n    console.log(newsCotent)\n\n})\n\nfor(let i =0; i <10; i++){\n    const li =document.createElement('li');\n    const a=document.createElement('a');\n\n    a.href=`#${newsFeed[i].id}`;\n    a.innerHTML=`${newsFeed[i].title} <b/> coment_count:${newsFeed[i].comments_count}`;\n\n    \n    li.appendChild(a);\n    ul.appendChild(li);\n}\n\n//html 하단에 자식 노드로 추가\ncontainer.appendChild(ul);\ncontainer.appendChild(content);\n\n//이벤트\n//json\n//함수\n//객체\n//api\n//컴파일\n//substriong\n//replace 함수 \n"},"lineCount":null}},"error":null,"hash":"8543f2172ef66d3f553dd04ead496869","cacheData":{"env":{}}}