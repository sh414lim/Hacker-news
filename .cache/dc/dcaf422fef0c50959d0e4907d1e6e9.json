":"app.js","original":{"line":87,"column":0}},{"name":"router","generated":{"line":57,"column":38},"source":"app.js","original":{"line":87,"column":37}},{"generated":{"line":57,"column":44},"source":"app.js","original":{"line":87,"column":0}},{"name":"router","generated":{"line":58,"column":0},"source":"app.js","original":{"line":89,"column":0}},{"generated":{"line":58,"column":6},"source":"app.js","original":{"line":89,"column":6}}],"sources":{"app.js":"const container=document.getElementById('root')\n\nconst ajax = new XMLHttpRequest();\nconst content=document.createElement('div')\nconst NEWS_URL ='https://api.hnpwa.com/v0/news/1.json';\nconst CONTENT_URL='https://api.hnpwa.com/v0/item/@id.json';\nconst store={\n    currentPage:1,\n}\n\n\nfunction getDate(url){ // ajax 함수\n    ajax.open('GET' ,url,false) //동기적 처리\n    ajax.send();//데이터 가져오기;\n    return JSON.parse(ajax.response);//json 을 객체로\n\n}\n\n\nfunction newsFead(){ //글 목록 함수\n    const newsFeed=getDate(NEWS_URL); \n    const newsList = [];\n    let template = `\n        <div>\n        <h1> Hacker News</h1>\n        <ul>\n        <li>\n        </li>\n        </ul>\n        <div>\n        <a href = \"#\">다음 페이지</a>\n        <a href = \"#\">이전 페이지</a>\n        </div>\n        </div>\n    `\n\n    newsList.push('<ul>');\n\n    for(let i =(store.currentPage - 1) * 10; i < store.currentPage * 10; i++){\n    newsList.push (`\n    <li>\n        <a href = '#/show/${newsFeed[i].id}'>\n        ${newsFeed[i].title} (${newsFeed[i].comments_count})\n        </a>\n    </li>`\n    );\n}\nnewsList.push('<ul/>');\nnewsList.push(`\n<div>\n <a href = #/page/${store.currentPage > 1 ? store.currentPage - 1 : 1}>이전 페이지 </a>\n <a href = #/page/${store.currentPage + 1}>다음 페이지 </a>\n</div>\n`)\n//html 하단에 자식 노드로 추가\ncontainer.innerHTML = newsList.join(''); //배열에 있는 문자열을 하나로 만든다 (join)\n}\n\n\n\n\nfunction newsDetail(){\n    //loaction 브라우저가 제공해주는 객체 ->주소와 관련된 다양한 정보를 알 수 있다\nconst id = location.hash.substr(7);//#제거\nconst newsCotent=getDate(CONTENT_URL.replace('@id',id))\n\ncontainer.innerHTML=`\n    <h1> ${newsCotent.title} </h1>\n        <div>\n            <a href =\"#/page/${store.currentPage}\">목록으로</a>\n        </div>\n`;\n}\nfunction router(){\n    const routePath = location.hash;\n    if(routePath === ''){\n        newsFead();\n    }else if(routePath.indexOf('#/page/' >= 0)){\n        store.currentPage =Number(routePath.substr(7));\n        newsFead();\n    }else{\n        newsDetail();\n\n    }\n}\n\nwindow.addEventListener('hashchange',router)\n\nrouter();"},"lineCount":null}},"error":null,"hash":"70cfb1375fd05cfb3f2ca3ab33502bdf","cacheData":{"env":{}}}